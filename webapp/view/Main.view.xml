<mvc:View controllerName="flightui5jn.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
  <Page id="page" title="{i18n>title}">
    <Table id="_IDGenTable1"
		items="{
			path: 'flightDataModel>/',
			sorter: {
				path: 'Carrname'
			}
		}">
		<headerToolbar>
			<OverflowToolbar id="_IDGenOverflowToolbar">
				<Title id="_IDGenTitle" text="{Products}" level="H2"/>
				<ToolbarSpacer id="_IDGenToolbarSpacer"/>
				<Button id="_IDGenButton" text="Open Dialog" press="onOpenPressed"/>
			</OverflowToolbar>
		</headerToolbar>
		<columns>
			<Column id="_IDGenColumn">
				<Text id="_IDGenText" text="{i18n>airlineId}" />
			</Column>
			<Column id="_IDGenColumn1">
				<Text id="_IDGenText1" text="{i18n>airlineName}" />
			</Column>
			<Column id="_IDGenColumn2">
				<Text id="_IDGenText2" text="{i18n>currency}" />
			</Column>
			<Column id="_IDGenColumn3">
				<Text id="_IDGenText3" text="{i18n>url}" />
			</Column>
		</columns>
		<items>
			<ColumnListItem id="_IDGenColumnListItem" vAlign="Middle" type="Navigation" press="onNavToEmpl" >
				<cells>
					<Text id="_IDGenText4" text="{flightDataModel>Carrid}" wrapping="false" />
					<Text id="_IDGenText5" text="{flightDataModel>Carrname}" wrapping="false"/>
					<Text id="_IDGenText7" text="{flightDataModel>Currency}" />
					<Text id="_IDGenText8" text="{flightDataModel>Url}" />
				</cells>
			</ColumnListItem>
		</items>
	</Table>
    </Page>
</mvc:View>