
<mvc:View controllerName="flightui5jn.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.uxap"
    xmlns:m="sap.m">
     <ObjectPageLayout id="ObjectPageLayout"
					 showTitleInHeaderContent="true"
					alwaysShowContentHeader="false"
					preserveHeaderStateOnScroll="false"
					headerContentPinnable="true"
					isChildPage="true"
					upperCaseAnchorBar="false">
				<headerTitle>
					<ObjectPageDynamicHeaderTitle id="_IDGenObjectPageDynamicHeaderTitle1">
						<expandedHeading>
							<m:Title id="_IDGenTitle2" text="{FlghDetailModel>/Carrname}" wrapping="true" class="sapUiSmallMarginEnd"/>
						</expandedHeading>

						<snappedHeading>
							<m:FlexBox id="_IDGenFlexBox" wrap="Wrap" fitContainer="true" alignItems="Center">
								<m:FlexBox id="_IDGenFlexBox1" wrap="NoWrap" fitContainer="true" alignItems="Center" class="sapUiTinyMarginEnd">
									<m:Title id="_IDGenTitle3" text="{FlghDetailModel>/Carrname}" wrapping="true" class="sapUiTinyMarginEnd"/>
								</m:FlexBox>
							</m:FlexBox>
						</snappedHeading>

						<navigationActions>
							<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton" type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"/>
							<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton1" type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"/>
							<m:OverflowToolbarButton id="_IDGenOverflowToolbarButton2" type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close column" visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"/>
						</navigationActions>

					</ObjectPageDynamicHeaderTitle>
				</headerTitle>

				<headerContent>
					<m:FlexBox id="_IDGenFlexBox2" wrap="Wrap" fitContainer="true" alignItems="Stretch">
						<m:Avatar id="_IDGenAvatar1" src="{path: 'FlghDetailModel>/Carrid', formatter:'.formatter.getCarrierLogo'}"
								  displaySize="L"
								  displayShape="Square"
								  class="sapUiTinyMarginEnd">
						</m:Avatar>
						<m:VBox id="_IDGenVBox" justifyContent="Center" class="sapUiSmallMarginEnd">
							<m:Label id="_IDGenLabel" text="Airline ID"/>
							<m:Text id="_IDGenText6" text="{FlghDetailModel>/Carrid}"/>
						</m:VBox>
						<m:VBox id="_IDGenVBox1" justifyContent="Center" class="sapUiSmallMarginEnd">
							<m:Label id="_IDGenLabel1" text="URL"/>
							<m:Text id="_IDGenText31" text="{
            parts: [
                {path: 'FlghDetailModel>/Url'},
                {path: 'FlghDetailModel>/Carrname'}
            ],
            formatter: '.formatter.getCarrierUrl'
        }"/>
						</m:VBox>
						<m:VBox id="_IDGenVBox2" justifyContent="Center" class="sapUiSmallMarginEnd">
							<m:Label id="_IDGenLabel2" text="Currency"/>
							<m:ObjectNumber id="_IDGenObjectNumber" number="{FlghDetailModel>/Currcode}" emphasized="false"/>
						</m:VBox>
					</m:FlexBox>
				</headerContent>

		<sections>
			<ObjectPageSection id="_IDGenObjectPageSection" title="General Information">
				<subSections>
					<ObjectPageSubSection id="_IDGenObjectPageSubSection">
						<blocks>
							 <m:Table id="_IDGenTable" 
								items="{
										path: 'FlghDetailModel>/to_flightdetailsJN/results'
									}">
		<m:headerToolbar>
			<m:OverflowToolbar id="_IDGenOverflowToolbar1" >
				<m:Title id="_IDGenTitle1"  text="{i18n>detailTitle}" level="H2"/>
				<m:ToolbarSpacer id="_IDGenToolbarSpacer1" />
			</m:OverflowToolbar>
		</m:headerToolbar> 
		<m:columns>
			<m:Column id="_IDGenColumn4" >
				<m:Text id="_IDGenText9"  text="{i18n>connNo}" />
			</m:Column>
			<m:Column id="_IDGenColumn5" >
				<m:Text id="_IDGenText10"  text="{i18n>flightDate}" />
			</m:Column>
			<m:Column id="_IDGenColumn6">
				<m:Text id="_IDGenText11" text="{i18n>airfare}" />
			</m:Column>
			<m:Column id="_IDGenColumn7">
				<m:Text id="_IDGenText12" text="{i18n>airlineLocalCurrency}" />
			</m:Column>
			<m:Column id="_IDGenColumn8">
				<m:Text id="_IDGenText13"  text="{i18n>maxTypeEco}" />
			</m:Column>
			<m:Column id="_IDGenColumn9">
				<m:Text id="_IDGenText14" text="{i18n>occSeatsEco}" />
			</m:Column>
			<m:Column id="_IDGenColumn10">
				<m:Text id="_IDGenText15" text="{i18n>maxTypeBsns}" />
			</m:Column>
			<m:Column id="_IDGenColumn11">
				<m:Text id="_IDGenText16" text="{i18n>occSeatsBsns}" />
			</m:Column>
			<m:Column id="_IDGenColumn12">
				<m:Text id="_IDGenText17" text="{i18n>maxTypeFrst}" />
			</m:Column>
			<m:Column id="_IDGenColumn13">
				<m:Text id="_IDGenText18" text="{i18n>occSeatsFrst}" />
			</m:Column>
		</m:columns>
		<m:items>
			<m:ColumnListItem id="_IDGenColumnListItem1" vAlign="Middle" highlight="{= ${FlghDetailModel>SeatsOcc} > 180 ? 'Success' : 'Error' }">
				<m:cells>
					<m:Text id="_IDGenText19" text="{FlghDetailModel>Connid}" wrapping="false" />
					<m:Text id="_IDGenText20"  text="{path:'FlghDetailModel>Fldate' , type: 'sap.ui.model.type.Date', formatOptions: {pattern: 'MM/dd/yyyy'}}" wrapping="false"/>
					<m:Text id="_IDGenText90"  text="{path: 'FlghDetailModel>Fldate', type: 'sap.ui.model.type.Date', formatOptions: {pattern: 'MM/dd/yyyy' }}" wrapping="false"/>
					<m:Text id="_IDGenText21"  text="{FlghDetailModel>Price}" />
					<m:Text id="_IDGenText22"  text="{FlghDetailModel>Currency}" />
					<m:Text id="_IDGenText23"  text="{FlghDetailModel>PlaneType}" />
					<m:Text id="_IDGenText24"  text="{FlghDetailModel>SeatsMax}" />
					<m:Text id="_IDGenText25"  text="{FlghDetailModel>SeatsOcc}" />
					<m:Text id="_IDGenText26"  text="{FlghDetailModel>PaymentSum}" />
					<m:Text id="_IDGenText27"  text="{FlghDetailModel>SeatsmaxB}" />
					<m:Text id="_IDGenText28"  text="{FlghDetailModel>SeatsoccB}" />
					<m:Text id="_IDGenText29"  text="{FlghDetailModel>SeatsmaxF}" />
					<m:Text id="_IDGenText30"  text="{FlghDetailModel>SeatsoccF}" />
				</m:cells>
			</m:ColumnListItem>
		</m:items>
	</m:Table>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>